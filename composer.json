{
    "name": "wyrihaximus/twig-view",
    "description": "Twig powered View for CakePHP3",
    "type":"cakephp-plugin",
    "license": "MIT",
    "keywords": [
        "twig",
        "view",
        "cakephp",
        "cakephp3"
    ],
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": ">=5.4.19",
        "cakephp/cakephp": "3.0.*-dev",
        "twig/twig": "^1.18",
        "cakephp/plugin-installer": "dev-master",
        "asm89/twig-cache-extension": "~1.0"
    },
    "require-dev": {
        "cakephp/debug_kit": "3.0.*@dev",
        "satooshi/php-coveralls": "^0.6",
        "phpunit/phpunit": "^4.0",
        "phake/phake": "^1.0",
        "wyrihaximus/phpunit-class-reflection-helpers": "dev-master",
        "vectorface/dunit": "^2.0",
        "squizlabs/php_codesniffer": "^1.0",
        "object-calisthenics/phpcs-calisthenics-rules": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\TwigView\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\CakePHP\\Tests\\TwigView\\": "tests/"
        }
    }
}
